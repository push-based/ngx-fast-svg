  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ],
        "accessToken": "ZmQ2MjlmYjctZjEzYy00YTIwLTk4NjgtZWM5MmY2Y2VhNmFlfHJlYWQtd3JpdGU="
      }
    }
  },
  "generators": {
